    "version": "0.2.0",
    "configurations": [
        {
            "name": "Student findAll",
            "request": "launch",
            "type": "dart",
            "program": "${workspaceFolder}/bin/adf_cli.dart",
            "args": [
                "students",
                "findAll"
            ]
        },
        {
            "name": "Student findById",
            "request": "launch",
            "type": "dart",
            "program": "${workspaceFolder}/bin/adf_cli.dart",
            "args":[
                "students",
                "byId",
                "-i",
                "1"
            ]
        },
    ]
}